void setup () {
  /** Sets the width and height of the canvas **/
  size(800, 400);
}

void draw() {
  
  /** Sets the background to a random color **/
  background(random(255), random(255), random(255));
  
  /** Sets a delay of 100 milliseconds **/
  delay(100);
}
